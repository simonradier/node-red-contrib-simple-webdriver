{
	"name": "node-red-contrib-selenium-wd2",
	"version": "0.2.0-alpha1",
	"description": "Selenium-webdriver nodes for Node-RED based on node-red-contrib-selenium-webdriver",
	"dependencies": {
		"rxjs": "~6.6.3"
	},
	"devDependencies": {
		"@types/node-red": "~1.1.0",
		"@types/node-red-node-test-helper": "~0.2.1",
		"prettier": "~2.1.2",
		"tslint": "~6.1.3",
		"tslint-config-prettier": "~1.18.0",
		"typescript": "~4.0.3",
		"typescript-tslint-plugin": "~0.5.5"
	},
	"license": "Apache-2.0",
	"keywords": [
		"node-red",
		"selenium-wd2"
	],
	"node-red": {
		"nodes": {
			"selenium-wd2": "dist/selenium-wd2.js"
		}
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/simonradier/node-red-contrib-selenium-wd2"
	},
	"engines": {
		"node": ">= 14.0.0"
	},
	"scripts": {
		"prepublishOnly": "mkdir dist && cp -r src/icons dist/icons && cat src/nodes/*.html > dist/selenium-wd2.html && tsc",
		"clean": "rm -rf dist"
	},
	"author": {
		"name": "Simon Radier"
	}
}
